<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="starcorp.common.entities.CashTransaction" table="ACCOUNT_LEDGER" lazy="false">
    	<id name="ID"><generator class="native"/></id>
    	<version name="version"/>
    	<property name="accountID"/>
    	<property name="amount" column="ctAmount"/>
    	<property name="description" column="ctDesc"/>
    	<property name="year" column="ctYear"/>
    	<property name="month" column="ctMonth"/>
    </class>
</hibernate-mapping>